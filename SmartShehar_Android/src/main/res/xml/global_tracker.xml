<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="TypographyDashes">
    <integer name="ga_sessionTimeout">300</integer>
    <!-- the Local LogLevel for Analytics -->
    <string name="ga_logLevel">verbose</string>

    <!-- how often the dispatcher should fire -->
    <integer name="ga_dispatchPeriod">30</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!-- The screen names that will appear in reports -->

    <screenName name="com.smartshehar.android.app.ui.ActTrainDashboard">Train Dashboard</screenName>
    <screenName name="com.smartshehar.android.app.ui.ActFares">Train Fares</screenName>
    <screenName name="com.smartshehar.android.app.ui.ActFastInfo">Train Fast Info</screenName>
    <screenName name="com.smartshehar.android.app.ui.ActTrainMap">Train Map</screenName>
    <screenName name="com.smartshehar.android.app.ui.ActTrainRoute">Train Route</screenName>
    <screenName name="com.smartshehar.android.app.ui.ActPref">Train Settings</screenName>
    <screenName name="com.smartshehar.android.app.ui.ActTrainTrip">Train Trip</screenName>
    <screenName name="com.smartshehar.android.app.ui.ActSendSMS">Train Send SMS</screenName>

    <!--  The following value should be replaced with correct property id. -->
    <string name="ga_trackingId">UA-32381506-5</string>
</resources>